     <script src="/js/script.js"></script>
     <script src="/js/jquery.js"></script>
</head>
<body>
    <style>
        .hidden {
            display: none;
        }
        .animate-spin {
            animation: spin 1s linear infinite;
        }
        @keyframes spin {
            0% {
                transform: rotate(0deg);
            }

            100% {
                transform: rotate(360deg);
            }
        }
    </style>
    {{{body}}}
</body>
</html>